{
    "name": "codeq/linkchecker",
    "description": "Finds broken and misconfigured links in your Neos project",
    "type": "neos-package",
    "license": "GPL-3.0-or-later",
    "require": {
        "league/csv": "^9.1",
        "neos/neos": "^4.3 || 5 - 8 || dev-master",
        "neos/swiftmailer": "^7.3.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "sitegeist/fusionlinkprototypes": "^1.0",
        "spatie/crawler": "^4.1",
        "symfony/polyfill-php80": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "CodeQ\\LinkChecker\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "CodeQ.LinkChecker"
        },
        "applied-flow-migrations": [
            "TYPO3.Form-20160601101500"
        ]
    }
}