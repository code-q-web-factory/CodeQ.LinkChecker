<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file original="" product-name="CodeQ.LinkChecker" source-language="de" datatype="plaintext">
        <body>
            <trans-unit id="module.label">
                <source>Link Überprüfung</source>
            </trans-unit>
            <trans-unit id="module.description">
                <source>Dieses Module listet fehlerhafte Links auf.</source>
            </trans-unit>

            <trans-unit id="pagination.position">
                <source>Zeige {0}-{1} von {2}</source>
            </trans-unit>

            <trans-unit id="list.action.delete">
                <source>Als erledigt markieren</source>
            </trans-unit>
            <trans-unit id="list.action.ignore">
                <source>Fehler ignorieren</source>
            </trans-unit>

            <trans-unit id="domain">
                <source>Ursprungsdomäne</source>
            </trans-unit>
            <trans-unit id="source">
                <source>Quellpfad</source>
            </trans-unit>
            <trans-unit id="targetUriPath">
                <source>Ziel-Url oder Pfad</source>
            </trans-unit>
            <trans-unit id="error">
                <source>Fehler</source>
            </trans-unit>
            <trans-unit id="error.0">
                <source>Timeout (0)</source>
            </trans-unit>
            <trans-unit id="error.301">
                <source>Dauerhaft verschoben (301)</source>
            </trans-unit>
            <trans-unit id="error.302">
                <source>Found (Moved Temporarily) (302)</source>
            </trans-unit>
            <trans-unit id="error.303">
                <source>Siehe anderswo (303)</source>
            </trans-unit>
            <trans-unit id="error.307">
                <source>Zeitweise Umleitung (307)</source>
            </trans-unit>
            <trans-unit id="error.308">
                <source>Dauerhafte Umleitung (308)</source>
            </trans-unit>
            <trans-unit id="error.400">
                <source>Ungültige Anfrage (400)</source>
            </trans-unit>
            <trans-unit id="error.401">
                <source>Unautorisiert (401)</source>
            </trans-unit>
            <trans-unit id="error.403">
                <source>Verboten (403)</source>
            </trans-unit>
            <trans-unit id="error.404">
                <source>Nicht gefunden (404)</source>
            </trans-unit>
            <trans-unit id="error.405">
                <source>Methode nicht erlaubt (405)</source>
            </trans-unit>
            <trans-unit id="error.408">
                <source>Anfrage-Zeitüberschreitung (408)</source>
            </trans-unit>
            <trans-unit id="error.429">
                <source>Zu viele Anfragen (429)</source>
            </trans-unit>
            <trans-unit id="error.490">
                <source>Telefonnummern sollten im internationalen Format angegeben werden (490)</source>
            </trans-unit>
            <trans-unit id="error.500">
                <source>Interner Server-Fehler (500)</source>
            </trans-unit>
            <trans-unit id="error.503">
                <source>Dienst nicht verfügbar (503)</source>
            </trans-unit>
            <trans-unit id="error.509">
                <source>Datenvolumen überschritten (509)</source>
            </trans-unit>
            <trans-unit id="foundAt">
                <source>Gefunden am</source>
            </trans-unit>

            <trans-unit id="action.run">
                <source>Neue überprüfen und zurücksetzen</source>
            </trans-unit>

            <trans-unit id="list.empty">
                <source>Es gibt keine fehlerhaften Links.</source>
            </trans-unit>

            <trans-unit id="noDomainsFound">
                <source>Es wurden keine Domains gefunden.</source>
            </trans-unit>

            <group id="errorsFound" restype="x-gettext-plurals">
                <trans-unit id="errorsFound[0]" >
                    <source>{amountOfErrors} ungültigen Link in der Domäne {domain} gefunden</source>
                </trans-unit>
                <trans-unit id="errorsFound[1]" >
                    <source>{amountOfErrors} ungültige Links in der Domäne {domain} gefunden</source>
                </trans-unit>
            </group>
        </body>
    </file>
</xliff>
